{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/ReturnPoint/bin/Debug/net6.0-windows/ReturnPoint.dll",
      "args": [],
      "cwd": "${workspaceFolder}/ReturnPoint",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "requireExactSource": false
    },
    {
      "name": ".NET Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}